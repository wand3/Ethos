{
  "version": 2,
  "builds": [
    {
      "src": "webapp/main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11", "buildCommand": "mkdir -p webapp/static" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webapp/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "webapp/static/$1"
    }
  ]
}
