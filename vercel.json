{
  "version": 2,
  "builds": [
    {
      "src": "webapp/main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webapp/main.py"
    },
    {
      "src": "webapp/static/(.*)",
      "dest": "webapp/static/$1"
    }
  ],
  "include": ["static"]
}
